
<section class="students col-sm-9">
	<h1 class="page-header">{title}</h1>
	<ul class="students-links">
		{#resources}
			<li>{text|s}</li>
		{/resources}
		<li>
			<p><u>Games:</u></p>
			<ul class="students-links-games">
				{#games}
					<li>{gameText|s}</li>	
				{/games}
			</ul>
		</li>
	</ul>
	<h2 class="students-apps-header">Music Apps for your Smartphone or Tablet</h2>
	<hr>
	<ul class="students-apps">
		{#apps}
			<li>{text}</li>
		{/apps}
	</ul>
</section>	

<div class="home">
	<a href="/" class="{routeClass}">
		<div class="home-container">
			<p>Return Home</p>
			<i class="fa fa-home fa-3x"></i>
		</div>
	</a>
</div>