{#posts}
	<div class="blog-posts-item">
		<h2 class="blog-posts-item-heading">{title}</h2>
		<p class="blog-posts-item-date"><u>{date}</u></p>
		{#content}
			<div class="blog-posts-item-content">
				{?contentTitle}
					<h3 class="content-title">{contentTitle}</h3>
				{:else}
					<p class="content-text">{text}</p>
				{/contentTitle}
				{video|s}
			</div>
			<hr>
		{/content}
	</div>
{/posts}